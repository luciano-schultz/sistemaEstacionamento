<?php

namespace Application\Entity;

/**
 * Preco
 */
class Preco
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var float
     */
    private $mensal;

    /**
     * @var float
     */
    private $horista;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set mensal
     *
     * @param float $mensal
     *
     * @return Preco
     */
    public function setMensal($mensal)
    {
        $this->mensal = $mensal;

        return $this;
    }

    /**
     * Get mensal
     *
     * @return float
     */
    public function getMensal()
    {
        return $this->mensal;
    }

    /**
     * Set horista
     *
     * @param float $horista
     *
     * @return Preco
     */
    public function setHorista($horista)
    {
        $this->horista = $horista;

        return $this;
    }

    /**
     * Get horista
     *
     * @return float
     */
    public function getHorista()
    {
        return $this->horista;
    }
}
